<?xml version="1.0"?>
<odoo>
    <!--   history Unit Price  -->
        <record id="history_property_area_price_tree" model="ir.ui.view">
            <field name="name">history.property.area.price</field>
            <field name="model">history.property.area.price</field>
            <field name="arch" type="xml">
                <tree string="Area Price">
                                    <field name="date"/>
                                    <field name="area_price"/>
                                    <field name="product_id"/>
                                    <field name="type_of_property_id"/>
                                    <field name="plot_area"/>
                                    <field name="price_m_a"/>
                                    <field name="state"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="history_property_area_price_action">
            <field name="name">History Area Price</field>
            <field name="res_model">history.property.area.price</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="history_property_area_price_menu"
            name="History Area Price"
            parent="add_real_estate.history_sub"
            action="history_property_area_price_action"
            sequence="1"/>
